#include <iostream>
#include<string>
using namespace std;
int main()
{
	int a, b;
	int i = 0;
	string s;
	cin >> a >> b;
	s = to_string(a + b);
	for(i=0;i<s.length();++i)
	{
		cout << s[i];
		if (s[i] == '-') continue;
		if ((i + 1) % 3 == s.length() % 3 && i != s.length() - 1)
			cout << ',';
	}
}
